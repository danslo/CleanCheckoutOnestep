<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <clean_checkout>
            <field_order>
                <firstname>20</firstname>
                <lastname>30</lastname>
                <street>40</street>
                <country_id>50</country_id>
                <region_id>60</region_id>
                <city>70</city>
                <postcode>80</postcode>
                <telephone>90</telephone>
            </field_order>
            <cleanup>
                <disable_header>1</disable_header>
                <disable_footer>1</disable_footer>
                <disable_copyright>1</disable_copyright>
                <display_logo>1</display_logo>
            </cleanup>
        </clean_checkout>
        <checkout>
            <options>
                <display_billing_address_on>1</display_billing_address_on>
            </options>
        </checkout>
    </default>
</config>